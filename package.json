    "@replit/extensions": "^1.8.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.21.0",
    "@types/cookie": "^0.5.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-toml": "^0.3.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.4.12"
  }
