{
  "name": "replit-ui-svelte-extension",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host=0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
